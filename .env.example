# ATM System Configuration
# Copy this file to .env and update with your values

# Security Configuration
ATM_SECURITY_SALT=change_this_to_a_random_string_for_production
ATM_SESSION_TIMEOUT=300
ATM_MAX_LOGIN_ATTEMPTS=3
ATM_LOCKOUT_DURATION=300

# System Configuration
ATM_DAILY_WITHDRAWAL_LIMIT=1000
ATM_LOGGING_LEVEL=INFO
ATM_DATA_FILE=atm_data.json

# Account Limits
CHECKING_DAILY_LIMIT=500
SAVINGS_DAILY_LIMIT=300
PREMIUM_DAILY_LIMIT=1000

# Overdraft Limits
CHECKING_OVERDRAFT=100
SAVINGS_OVERDRAFT=0
PREMIUM_OVERDRAFT=500
